<div class="container border">
     <ul class = "border"> <%= @restaurant.name %> </ul>
      <ul class = "border"> <%= @restaurant.address %> </ul>
       <ul class = "border"> <%= @restaurant.category %> </ul>
          <% @restaurant.reviews.each do |e| %>
                <ul class = "border"> <%= e.content %> </ul>
                <% end %>
  <h2> <%= link_to 'Add a LETS GOOOOO', new_restaurant_review_path(@restaurant) %> </h2>

  <h2> <%= link_to 'Back', restaurants_path %> </h2>
</div>
